{{ template "base" .}}
{{ define "content"}}
<div class="container">
  <div class="row">
    <div class="col">
      <h1>This is the About page</h1>
      <p>This came from the template: {{ index .StringMap "test" }}</p>
      <p>
        {{if ne (index .StringMap "remote_ip") ""}}
        Your Remote IP address is: {{ index .StringMap "remote_ip" }}
        {{ else }}
        I don't know you IP address yet. Visi the <a href="/">Home Page</a> so I
        can set it
        {{ end }}
      </p>
    </div>
  </div>
</div>
{{ end }}
