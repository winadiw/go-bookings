{{ template "base" .}}
{{ define "content"}}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1 class="mt-3">Choose a room</h1>
                {{ $rooms := index .Data "rooms" }}
                {{ range $rooms }}
                    {{ .RoomName }}<br>
                {{ end }}
            </div>
        </div>
    </div>
{{ end }}